<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.iyeongjun.gucknaesan">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".ForestApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity android:name=".ui.activities.main.MainActivity" />
        <activity android:name=".ui.activities.splash.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.activities.detail.DetailActivity" />
        <activity android:name=".ui.activities.cal.CalActivity" />
        <activity android:name=".ui.activities.club.ClubDetailActivity" />
        <activity android:name=".ui.activities.clubCar.ClubCarActivity" />
        <activity android:name=".ui.activities.tuto.TutoActivity"></activity>
    </application>

</manifest>